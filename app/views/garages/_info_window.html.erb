<div class="d-flex flex-column map_popup">
  <div class="d-flex flex-row justify-content-between">
    <% if garage.photo.attached? %>
      <%= cl_image_tag garage.photo.key, height: 60, width: 60, crop: :fill, class: " logo_map" %>
    <% end %>

    <div class="map_price d-flex flex-row">
      <% if garage.services.first != nil%>
        <%=garage.services.first.price%>
        <div class="auros"><i class="fas fa-euro-sign"></i></div>
      <%end%>
    </div>
  </div>

  <h2><%=garage.name%></h2>
</div>
